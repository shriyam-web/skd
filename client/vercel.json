{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "https://skd-backend.onrender.com/sitemap.xml"
    },
    {
      "source": "/api/:path*",
      "destination": "https://skd-backend.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
